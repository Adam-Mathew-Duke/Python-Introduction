# lists with strings
shopping_list = ['apple','orange','bananas','cheese']
print(shopping_list[0:2])
shopping_list.append('blueberries')
shopping_list[0] = 'cherries'
del shopping_list[1]
print(shopping_list[0:])
print(len(shopping_list))
shopping_list2 = ['bread','jam','butter']
print (shopping_list2)
print (shopping_list + shopping_list2)
print (shopping_list * 2)

# lists with numbers
list_num = [4,7,23,6]
print (list_num)
print (max(list_num))
print (min(list_num))
